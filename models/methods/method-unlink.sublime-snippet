@api.multi
def unlink(self):
    """
        Delete all record(s) from recordset
        return True on success, False otherwise

        @return: True on success, False otherwise

        #TODO: process before delete resource
    """

    result = super(${1:ModelName}, self).unlink()${0}

    return result
